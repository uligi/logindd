@model loginregistermenu.Models.Empleado

@{
    ViewData["Title"] = "Registrar Empleado";
}

<h2>Registrar Empleado</h2>

<form asp-action="RegistrarEmpleado" method="post">
    <div class="form-group">
        <label asp-for="Nombre"></label>
        <input asp-for="Nombre" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Apellido"></label>
        <input asp-for="Apellido" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Correo"></label>
        <input asp-for="Correo" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Posicion"></label>
        <input asp-for="Posicion" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Registrar</button>
</form>

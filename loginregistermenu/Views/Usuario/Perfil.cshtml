@model loginregistermenu.Models.Usuario

@{
    ViewData["Title"] = "Perfil";
}

<h2>Perfil de @Model.Nombre</h2>

<div class="form-group">
    <label>Nombre:</label>
    <span>@Model.Nombre</span>
</div>
<div class="form-group">
    <label>Cédula:</label>
    <span>@Model.Cedula</span>
</div>
<div class="form-group">
    <label>Correo:</label>
    <span>@Model.Correo</span>
</div>
<!-- Añade más campos aquí si es necesario -->
